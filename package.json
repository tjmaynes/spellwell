{
  "name": "spellwell",
  "version": "0.1.0",
  "type": "module",
  "description": "A modern vocabulary learning application with multiple game modes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmaynes/spellwell.git"
  },
  "homepage": "https://github.com/tjmaynes/spellwell#readme",
  "bugs": {
    "url": "https://github.com/tjmaynes/spellwell/issues"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,astro}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md,astro}\"",
    "cf:dev": "wrangler dev",
    "cf:deploy": "wrangler deploy",
    "cf:staging": "wrangler deploy --env staging",
    "cf:production": "wrangler deploy --env production"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@nanostores/react": "^1.0.0",
    "astro": "^5.16.4",
    "nanostores": "^1.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "wrangler": "^4.53.0"
  }
}
